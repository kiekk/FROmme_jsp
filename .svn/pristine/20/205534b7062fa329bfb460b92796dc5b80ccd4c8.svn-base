package com.fromme.app.board.vo;

public class ReplyVO {
	private int post_no;
	private int replys_no;
	private String replys_id;
	private String replys_content;
	private String replys_date;
	
	public int getPost_no() {
		return post_no;
	}
	public void setPost_no(int post_no) {
		this.post_no = post_no;
	}
	public int getReplys_no() {
		return replys_no;
	}
	public void setReplys_no(int replys_no) {
		this.replys_no = replys_no;
	}
	public String getReplys_id() {
		return replys_id;
	}
	public void setReplys_id(String replys_id) {
		this.replys_id = replys_id;
	}
	public String getReplys_content() {
		return replys_content;
	}
	public void setReplys_content(String replys_content) {
		this.replys_content = replys_content;
	}
	public String getReplys_date() {
		return replys_date;
	}
	public void setReplys_date(String replys_date) {
		this.replys_date = replys_date;
	}
	
	
	
}


